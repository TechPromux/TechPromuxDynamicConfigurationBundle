<?php

namespace TechPromux\Bundle\ConfigurationBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * OwnerConfiguration
 *
 * @ORM\Table(name="techpromux_owner_configuration")
 * @ORM\Entity()
 */
class OwnerConfiguration
{

    /**
     * @var string
     *
     * @ORM\Column(name="value", type="text")
     */
    private $value;

    /**
     * @ORM\ManyToOne(targetEntity="TechPromux\Bundle\ConfigurationBundle\Entity\CustomConfiguration")
     * @ORM\JoinColumn(name="configuration_id", referencedColumnName="id", nullable=false)
     */
    private $configuration;

    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set value
     *
     * @param string $value
     *
     * @return OwnerConfiguration
     */
    public function setValue($value)
    {
        $this->value = $value;

        return $this;
    }

    /**
     * Get value
     *
     * @return string
     */
    public function getValue()
    {
        return $this->value;
    }
}

